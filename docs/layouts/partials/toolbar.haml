#breadcrumbs
  %ul
    - breadcrumbs_trail.each_with_index do |bc, index|
      - next if index == 0
      - next unless bc
      - klass = "first" if index == 1
      - klass = "last" if index == breadcrumbs_trail.size - 1
      %li{ :class => klass }
        = link_to_unless_current( bc[:title], bc )

