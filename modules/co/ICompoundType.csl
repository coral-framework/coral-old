/*
	Base interface for Types that may have 'members' (i.e. attributes, methods, etc.).
 */
interface ICompoundType : IType
{
	/*
		List of all members of this type. A member can be an attribute, a method, etc.
	 */
	readonly attribute IMemberInfo[] members;

	/*
		Retrieves the member with the given name, if one exists; otherwise, returns null.
		For types that support inheritance, this method should consider inherited members.
	 */
	IMemberInfo getMember( in string name );
};
