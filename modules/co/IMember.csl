/*
	Interface for the \e members (i.e. fields, methods and ports) of a composite type.
 */
interface IMember
{
	// Member name, which must be unique within a ICompositeType.
	readonly attribute string name;

	// ICompositeType that contains the member.
	readonly attribute ICompositeType owner;

	// Index of the member within its owner's \a members array.
	readonly attribute uint16 index;
};
