/*
	The blueprint of a component.
 */
interface ComponentType : CompoundType
{
	// List of all member interfaces (both facets and receptacles) of this component.
	readonly attribute InterfaceInfo[] interfaces;

	// List of all facets (server interfaces) of this component.
	readonly attribute InterfaceInfo[] facets;

	// List of all receptacles (client interfaces) of this component.
	readonly attribute InterfaceInfo[] receptacles;
};
