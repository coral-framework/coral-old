/*
	The blueprint of a component.
 */
interface IComponent : ICompositeType
{
	// List of all ports (i.e. facets and receptacles) of this component.
	readonly attribute IPort[] ports;

	// List of all facets (provided interfaces) of this component.
	readonly attribute IPort[] facets;

	// List of all receptacles (required interfaces) of this component.
	readonly attribute IPort[] receptacles;
};
