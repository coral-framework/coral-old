/*
	Offers type introspection for a method, as a member of a IClassType.
 */
interface IMethod : IMember
{
	// The method's return type.
	readonly attribute IType returnType;

	// The list of parameters that this method takes.
	readonly attribute IParameter[] parameters;

	// The list of exceptions that might be thrown by this method.
	readonly attribute IException[] exceptions;
};
