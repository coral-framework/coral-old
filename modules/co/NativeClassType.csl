/*
	Type representing a native class (a C++ class wrapped as a Coral value).
 */
interface NativeClassType : CompoundType, AttributeContainer, MethodContainer
{
	// Fully-qualified name of the C++ type wrapped by this native class.
	readonly attribute string nativeName;

	// Name of the header file where the wrapped C++ type is defined.
	readonly attribute string nativeHeaderFile;
};
