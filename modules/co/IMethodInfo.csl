/*
	Offers type introspection for a method, as a member of a IMethodContainer.
 */
interface IMethodInfo : IMemberInfo
{
	// The method's return type.
	readonly attribute IType returnType;

	// The list of parameters that this method takes.
	readonly attribute IParameterInfo[] parameters;

	// The list of exceptions that might be thrown by this method.
	readonly attribute IExceptionType[] exceptions;
};
