/*
	Base interface for members of a CompoundType (i.e. an attribute, method, interface, etc.).
 */
interface MemberInfo
{
	// The name of the member.
	readonly attribute string name;

	// The CompoundType that this member belongs to.
	readonly attribute CompoundType owner;

	// This member's index within its owner's \a members array.
	readonly attribute uint32 index;
};
